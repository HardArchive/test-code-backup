        <TR>
          <TD vAlign=3Dtop =
align=3Dleft>WM_XHTMLTREE_DISPLAY_TOOLTIP</TD>
          <TD vAlign=3Dtop align=3Dleft>Sent when a tooltip is about to =
be=20
            displayed</TD>
          <TD vAlign=3Dtop align=3Dleft>Pointer to tooltip control=20
            (<CODE>CToolTipCtrl</CODE> or =
<CODE>CPPToolTip</CODE>)</TD></TR>
        <TR bgColor=3D#ddf0fe>
          <TD vAlign=3Dtop align=3Dleft>WM_XHTMLTREE_INIT_TOOLTIP</TD>
          <TD vAlign=3Dtop align=3Dleft>Sent when tooltip control is =
being=20
            initialized</TD>
