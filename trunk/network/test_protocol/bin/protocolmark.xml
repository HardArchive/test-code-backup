  <!--baidu标记类型-->
  <FindInfo>
     <!--是否需要延伸查找(即在第二个包查找)  1是  0否-->
    <IsExtendFind>0</IsExtendFind>
    <!--服务类型,与enum ApplicationProtocolType相对应-->
    <ServicesType>3014</ServicesType>
    <!--域名 当类型为非http协议时为空 最长度为128位-->
    <Host>Host: www.baidu.com</Host>
    <!--包头标记  最长度为64位-->    
    <PacketHeadTag>GET /cache/user/html/jump.html?</PacketHeadTag>
    <!--查找标志链表 此表不限长度-->
    <MarkFind>
      <!--codetype编码类型 URL GB1212 UTF_8 UNICODE 因为有的数据前后标记不一样 markstart开始标记 markend结束标记 savetype(数据保存类型)-->
      <MFRK codetype ="1" markstart ="username=" markend = "amp" savetype="1"/>
    </MarkFind>
  </FindInfo>


  <!--BBSMaopu 帐号密码-->
  <FindInfo>
    <IsExtendFind>1</IsExtendFind>
    <ServicesType>3045</ServicesType>
    <Host>Host: login.hi.mop.com</Host>
    <PacketHeadTag>/Login.do HTTP/1.1\r\n</PacketHeadTag>
    <MarkFind>
      <MFRK codetype ="2" markstart ="nickname=" markend = "amp" savetype="1"/> 
      <MFRK codetype ="2" markstart ="password=" markend = "amp" savetype="1"/>
    </MarkFind>
  </FindInfo>

  
  <!--配置-->
  <config>
    <Settings isshanghai="true"/>>    
  </config>
</root>
